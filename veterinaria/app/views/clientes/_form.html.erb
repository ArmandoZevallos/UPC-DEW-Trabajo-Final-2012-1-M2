<%= form_for(@cliente) do |f| %>
  <% if @cliente.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@cliente.errors.count, "error") %> prohibited this cliente from being saved:</h2>

      <ul>
      <% @cliente.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :nombre_cliente %><br />
    <%= f.text_field :nombre_cliente %>
  </div>
  <div class="field">
    <%= f.label :correo_cliente %><br />
    <%= f.text_field :correo_cliente %>
  </div>
  <div class="field">
    <%= f.label :imagen_cliente %><br />
	<input type="file" id="cliente_imagen_cliente" name="cliente[imagen_cliente]" size="40">
  </div>
  <div class="field">
    <%= f.label :tel_casa_cliente %><br />
    <%= f.text_field :tel_casa_cliente %>
  </div>
  <div class="field">
    <%= f.label :tel_cel_cliente %><br />
    <%= f.text_field :tel_cel_cliente %>
  </div>
  <div class="field">
    <%= f.label :Prospecto %><br />
    <%= f.collection_select :Prospecto_id, Prospecto.all, :id, :nombre_prospecto, :prompt => true %>
  </div>
  
  
  
  <div class="field">
    <%= f.label :Doctor %><br />
   
	<%= f.collection_select :Doctor_id, Doctor.all, :id, :nombre, :prompt => true %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>

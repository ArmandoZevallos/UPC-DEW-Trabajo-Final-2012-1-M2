 <h1>Listing doctors</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Apellido</th>
    <th>Correo</th>
    <th>Estado</th>
    <th>Users</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @doctors.each do |doctor| %>
  <tr>
    <td><%= doctor.nombre %></td>
    <td><%= doctor.apellido %></td>
    <td><%= doctor.correo %></td>
    <td><%= doctor.Estado.descripcion %></td>
    <td><%= doctor.Users.email  %></td>
    <td><%= link_to 'Show', doctor %></td>
    <td><%= link_to 'Edit', edit_doctor_path(doctor) %></td>
    <td><%= link_to 'Destroy', doctor, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>


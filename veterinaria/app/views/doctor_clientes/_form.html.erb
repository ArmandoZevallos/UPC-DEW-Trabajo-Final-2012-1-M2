      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :nota_cliente %><br />
    <%= f.text_area :nota_cliente %>
  </div>
  <div class="field">
    <%= f.label :fecha_doctorcliente %><br />
    <%= f.date_select :fecha_doctorcliente %>
